{
  "name": "js-algos",
  "version": "1.0.0",
  "description": "A Repo of algorithms written in javascript and explained",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "flow-typed": "flow-typed install",
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix; exit 0",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.js'",
    "spec": "jest",
    "postinstall": "npm run flow-typed",
    "test": "npm run spec"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amilajack/js-algorithims.git"
  },
  "author": "Amila Welihinda",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amilajack/algorithims/issues"
  },
  "homepage": "https://github.com/amilajack/algorithims#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-fail-explicit": "^0.0.6",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "eslint": "^5.0.1",
    "eslint-config-bliss": "^2.0.0",
    "eslint-formatter-pretty": "^1.3.0",
    "flow-bin": "^0.78.0",
    "flow-typed": "^2.4.0",
    "husky": "^1.0.0-rc.1",
    "jest-cli": "^23.2.0"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  }
}
